{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-spawn",
    "shell:allow-execute",
    "shell:allow-kill",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "clipboard-manager:allow-write-text",
    "clipboard-manager:allow-read-text",
    "process:allow-exit",
    "process:allow-restart"
  ]
}
